model User {
    id                  String   @id @default(uuid())
    user_login          String
    user_pass           String
    user_nicename       String?
    user_email          String
    user_url            String?
    user_registered     DateTime
    user_activation_key String?
    user_status         Int      @default(0)
    display_name        String?
    created_at          DateTime @default(now())
    updated_at          DateTime @updatedAt

    @@map("users")
}
